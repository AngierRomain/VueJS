{"remainingRequest":"/Users/simom/Documents/LP-LOCAL/Vuejs/td3/vue_dawin/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/simom/Documents/LP-LOCAL/Vuejs/td3/vue_dawin/src/App.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"/Users/simom/Documents/LP-LOCAL/Vuejs/td3/vue_dawin/src/App.vue","mtime":1607677130551},{"path":"/Users/simom/Documents/LP-LOCAL/Vuejs/td3/vue_dawin/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/simom/Documents/LP-LOCAL/Vuejs/td3/vue_dawin/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/simom/Documents/LP-LOCAL/Vuejs/td3/vue_dawin/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/simom/Documents/LP-LOCAL/Vuejs/td3/vue_dawin/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/simom/Documents/LP-LOCAL/Vuejs/td3/vue_dawin/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["App.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiNA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"App.vue","sourceRoot":"src","sourcesContent":["<template>\n    <div id=\"app\">\n        <img alt=\"Vue logo\" src=\"./assets/logo.png\" />\n\n        <div>\n            <div>\n                <input v-model=\"rechercheValue\" />\n                <button v-on:click=\"recherche\">Rechercher</button>\n                <button v-on:click=\"reinitRecherche\">Reinitialiser</button>\n            </div>\n            <p>{{ nbMovie }} films</p>\n            <ul>\n                <li\n                    class=\"movie\"\n                    v-for=\"(movie, i) in movies\"\n                    v-bind:key=\"i\"\n                    v-bind:title=\"movie.title\"\n                >\n                    <movieitem\n                        v-bind:movie=\"movie\"\n                        v-on:calldodisplaysynopsis=\"doDisplaySynopsis\"\n                        v-on:calledit=\"displayEdit\"\n                        v-on:calldelete=\"deleteMovie\"\n                    ></movieitem>\n                </li>\n            </ul>\n\n            <div>\n                <button v-on:click=\"displayAdd\">Ajouter un film</button>\n            </div>\n            <div v-if=\"action == 'add'\">\n                <h3>Ajouter un film</h3>\n                <input v-on:input=\"checkValue\" v-model=\"dataTitle\" />\n                <input v-on:input=\"checkValue\" v-model=\"dataReleaseDate\" />\n                <input v-on:input=\"checkValue\" v-model=\"dataSynopsis\" />\n                <input v-on:input=\"checkValue\" v-model=\"dataDirector\" />\n                <button v-on:click=\"addMovie\">Sauvegarder le film</button>\n                <p v-if=\"displayResume\">\n                    {{ dataTitle }} ({{ dataReleaseDate }}): {{ dataSynopsis }},\n                    {{ dataDirector }}\n                </p>\n            </div>\n            <div v-if=\"action == 'edit'\">\n                <h3>Modifier un film</h3>\n                <input v-on:input=\"checkValue\" v-model=\"dataTitle\" />\n                <input v-on:input=\"checkValue\" v-model=\"dataReleaseDate\" />\n                <input v-on:input=\"checkValue\" v-model=\"dataSynopsis\" />\n                <input v-on:input=\"checkValue\" v-model=\"dataDirector\" />\n                <button v-on:click=\"editMovie\">Sauvegarder le film</button>\n                <p v-if=\"displayResume\">\n                    {{ dataTitle }} ({{ dataReleaseDate }}): {{ dataSynopsis }},\n                    {{ dataDirector }}\n                </p>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n//import HelloWorld from \"./components/HelloWorld.vue\";\nimport movieitem from \"./components/movieitem.vue\";\n\nexport default {\n    name: \"App\",\n    components: {\n        movieitem,\n    },\n    data: function() {\n        return {\n            dataTitle: \"\",\n            dataReleaseDate: \"\",\n            dataDirector: \"\",\n            dataSynopsis: \"\",\n            displayResume: false,\n            displayMovie: true,\n            action: \"\",\n            cacheMovie: null,\n            rechercheValue: \"\",\n            movies: [\n                {\n                    title: \"Interstellar\",\n                    releaseDate: 2014,\n                    director: \"Christopher Nolan\",\n                    synopsis: \"Un film qui parle de physique et d'amour\",\n                    displaySynopsis: false,\n                    displayMovie: true,\n                },\n                {\n                    title: \"Tenet\",\n                    releaseDate: 2020,\n                    director: \"Christopher Nolan\",\n                    synopsis: \"Un film qui parle de temps\",\n                    displaySynopsis: false,\n                    displayMovie: true,\n                },\n            ],\n        };\n    },\n    methods: {\n        doDisplaySynopsis: function(aMovie) {\n            if (aMovie.displaySynopsis == true) {\n                aMovie.displaySynopsis = false;\n            } else {\n                aMovie.displaySynopsis = true;\n            }\n        },\n        displayAdd: function() {\n            this.action = \"add\";\n            this.dataTitle = \"\";\n            this.dataReleaseDate = \"\";\n            this.dataDirector = \"\";\n            this.dataSynopsis = \"\";\n            this.displayResume = false;\n            this.displayMovie = true;\n        },\n        addMovie: function() {\n            let movie = {\n                title: this.dataTitle,\n                releaseDate: this.dataReleaseDate,\n                director: this.dataDirector,\n                synopsis: this.dataSynopsis,\n                displaySynopsis: false,\n                displayMovie: true,\n            };\n            this.movies.push(movie);\n            this.dataTitle = \"\";\n            this.dataReleaseDate = \"\";\n            this.dataDirector = \"\";\n            this.dataSynopsis = \"\";\n            this.displayResume = false;\n            this.displayMovie = true;\n            this.action = \"\";\n        },\n\n        displayEdit: function(aMovie) {\n            this.action = \"edit\";\n            this.dataTitle = aMovie.title;\n            this.dataReleaseDate = aMovie.releaseDate;\n            this.dataDirector = aMovie.director;\n            this.dataSynopsis = aMovie.synopsis;\n            this.displayResume = true;\n            this.cacheMovie = aMovie;\n        },\n        editMovie: function() {\n            console.log(this.cacheMovie);\n            this.cacheMovie.title = this.dataTitle;\n            this.cacheMovie.releaseDate = this.dataReleaseDate;\n            this.cacheMovie.director = this.dataDirector;\n            this.cacheMovie.synopsis = this.dataSynopsis;\n            this.action = \"\";\n        },\n        deleteMovie: function(indice) {\n            this.movies.splice(indice, 1);\n        },\n        checkValue: function() {\n            let test = true;\n            if (this.dataTitle == \"\") {\n                test = false;\n            }\n            if (this.dataReleaseDate == \"\") {\n                test = false;\n            }\n            if (this.dataDirector == \"\") {\n                test = false;\n            }\n            if (this.dataSynopsis == \"\") {\n                test = false;\n            }\n\n            if (test == false) {\n                this.displayResume = false;\n            } else {\n                this.displayResume = true;\n            }\n        },\n        recherche: function() {\n            let value = this.rechercheValue;\n            let moviesSearched = this.movies;\n            let good;\n            moviesSearched.forEach((movie) => {\n                good = false;\n                if (\n                    movie.title == value ||\n                    movie.releaseDate == value ||\n                    movie.director == value\n                ) {\n                    good = true;\n                }\n                if (good == false) {\n                    movie.displayMovie = false;\n                }\n            });\n        },\n        reinitRecherche: function() {\n            let moviesSearched = this.movies;\n            moviesSearched.forEach((movie) => {\n                movie.displayMovie = true;\n            });\n        },\n    },\n    computed: {\n        nbMovie: function() {\n            return this.movies.length;\n        },\n    },\n};\n</script>\n\n<style>\n#app {\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    text-align: center;\n    color: #2c3e50;\n    margin-top: 60px;\n}\nli {\n    list-style: none;\n}\nul {\n    padding: 10px 0;\n}\n</style>\n"]}]}